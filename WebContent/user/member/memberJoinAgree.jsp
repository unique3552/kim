<title>어라운드 무비</title>
<script type="text/javascript">
	$(function() {
		var check1 = false;
		var check2 = false;
		var check3 = false;
		
		$("#chkAgree").change(function() {
			if ($(this).is(":checked")) {
				$('input:radio[name="agree1"]:input[value="y"]').prop("checked", true);
				$('input:radio[name="agree2"]:input[value="y"]').prop("checked", true);
				$('input:radio[name="agree3"]:input[value="y"]').prop("checked", true);
				check1 = true;
				check2 = true;
				check3 = true;
			} else {
				$('input:radio[name="agree1"]:input[value="y"]').prop("checked", false);
				$('input:radio[name="agree2"]:input[value="y"]').prop("checked", false);
				$('input:radio[name="agree3"]:input[value="y"]').prop("checked", false);
				check1 = false;
				check2 = false;
				check3 = false;
			}
		});
		
		$('input:radio[name="agree1"]').change(function() {
			if ($(':input[value="y"]').is(":checked")) {
				check1 = true;
			} else if ($(':input[value="n"]').is(":checked")) {
				check1 = false;
			}
		});
		
		$('input:radio[name="agree2"]').change(function() {
			if ($(':input[value="y"]').is(":checked")) {
				check2 = true;
			} else if ($(':input[value="n"]').is(":checked")) {
				check2 = false;
			}
		});
		
		$('input:radio[name="agree3"]').change(function() {
			if ($(':input[value="y"]').is(":checked")) {
				check3 = true;
			} else if ($(':input[value="n"]').is(":checked")) {
				check3 = false;
			}
		});
		
		$("#cancel").click(function() {
			location.href="../../index.jsp";
		});
		
		$("#next").click(function() {			
			if (check1 == true && check2 == true && check3 == true) {
				location.href="./memberJoin.jsp";
			} else {
				alert("약관에 모두 동의해 주세요.");
			}
		});
	});
</script>
